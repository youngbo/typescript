{
  "devDependencies": {
    "@types/jest": "^29.0.3",
    "jest": "^29.0.3",
    "supertest": "^6.2.4",
    "ts-jest": "^29.0.2",
    "typescript": "^4.8.3"
  },
  "jest": {
    "transform": {
        "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "\\.test\\.ts$",
    "moduleFileExtensions": [
        "ts",
        "tsx",
        "js",
        "json"
    ],
    "globals": {
        "ts-jest": {
            "diagnostics": true
        }
    }
},
  "scripts": {
    "test": "jest --detectOpenHandles --forceExit"
  }
}
